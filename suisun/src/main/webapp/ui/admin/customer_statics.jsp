<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%><%@ taglib uri="/WEB-INF/tlds/c.tld" prefix="c"%><%@ taglib prefix="form" uri="http://www.springframework.org/tags/form"%><%	String path = request.getContextPath();			String basePath = request.getScheme() + "://"					+ request.getServerName() + ":" + request.getServerPort()					+ path + "/";%><head><base href="<%=basePath%>" /><meta charset="utf-8"><title>随商管理后台</title><link media="all" rel="stylesheet" type="text/css"	href="ui/css/main.css" /><link media="all" rel="stylesheet" type="text/css"	href="ui/css/table.css" /><script type="text/javascript" src="ui/js/jquery/jquery-1.7.2.min.js"></script><script type="text/javascript" src="ui/js/pageCusStatics.js"></script><script type="text/javascript" src="ui/js/customer_statics.js"></script><!--[if lt IE 9]><link rel="stylesheet" type="text/css" href="css/ie.css" /><![endif]--></head><body>	<div id="wrapper">		<div id="content">			<div class="c1">				<%@include file="common/head.jsp"%>				<div id="right_content">					<h2>						<label>客户管理</label> <label style="float: right;"><a							href="javascript:void(0);" class="button greens" id="searchBtn">搜索</a>						</label> 						<label style="float: right; margin-right: 10px;">排序:<select id="orderBy">								<option value="3"									<c:if test="${orderBy == 3 }"> selected="selected"</c:if>>打开次数</option>								<option value="1"									<c:if test="${orderBy == 1 }"> selected="selected"</c:if>>下载时间</option>								<option value="2"									<c:if test="${orderBy == 2 }"> selected="selected"</c:if>>最近查看时间</option>						</select> </label> 												<label style="float: right;margin-right: 10px;"><select id="album">						      <option value="">所有画册</option>						      <c:forEach items="${alList}" var="a">								<option value="${a.albumName}" <c:if test="${a.albumName == album}">selected="selected"</c:if>>${a.albumName}</option>						      						      </c:forEach>														</select> </label>																		<label style="float: right; margin-right: 10px;"><select id="albumType">								<option value=""									<c:if test="${albumType == ''}"> selected="selected"</c:if>>所有平台</option>								<option value="mobile"									<c:if test="${albumType == 'mobile'}"> selected="selected"</c:if>>mobile</option>								<option value="pad"									<c:if test="${albumType == 'pad'}"> selected="selected"</c:if>>pad</option>						</select> </label>					</h2>					<table id="rounded-corner">						<thead>							<tr>								<th>客户机型</th>								<th>所属地区</th>								<th>查看画册</th>								<th>画册类型</th>								<th>打开次数</th>								<th>下载时间</th>								<th>最近查看时间</th>							</tr>						</thead>						<tfoot>							<tr>								<td colspan="12">									<div style="width: 400px;margin-top: 10px;float: right;">										<input type="hidden" value="${currentPage}" id="current">										<input type="hidden" value="${pageAll}" id="pageAll">										当前第${currentPage}页&nbsp;&nbsp;一共${pageAll}页&nbsp;&nbsp; <input											type="button" value="上一页" onclick="page.per();" />&nbsp;&nbsp;										<input type="button" value="下一页" onclick="page.next()" />&nbsp;&nbsp;										<input type="text" style="width: 30px;"											value="${currentPage }" name="currentPage" />&nbsp;&nbsp; <input											type="button" value="确认" onclick="page.jump();" />&nbsp;&nbsp;									</div>								</td>							</tr>						</tfoot>						<tbody>							<c:forEach items="${csList}" var="u" varStatus="status">								<tr id="${u.uuid}"									<c:if test="${status.count%2 == 1}">								   class="even"								</c:if>									<c:if test="${status.count%2 == 0}">									class="odd"								</c:if>>									<td>${u.phoneType}</td>									<td>${u.area}</td>									<td>${u.albumLook}</td>									<td>${u.albumType}</td>									<td>${u.openNum}</td>									<td>${u.downloadTime}</td>									<td>${u.lastTimeLook}</td>								</tr>							</c:forEach>						</tbody>					</table>				</div>			</div>		</div>		<%@include file="common/left.jsp"%>	</div></body></html>