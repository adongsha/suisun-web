<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%><%@ taglib uri="/WEB-INF/tlds/c.tld" prefix="c"%><%@ taglib prefix="form" uri="http://www.springframework.org/tags/form"%><%	String path = request.getContextPath();			String basePath = request.getScheme() + "://"					+ request.getServerName() + ":" + request.getServerPort()					+ path + "/";%><head><base href="<%=basePath%>" /><meta charset="utf-8"><title>随商管理后台</title><link media="all" rel="stylesheet" type="text/css"	href="ui/css/main.css" /><link media="all" rel="stylesheet" type="text/css"	href="ui/css/table.css" /><link rel="stylesheet" href="ui/js/jquery/css/jquery-ui.css"><script type="text/javascript" src="ui/js/jquery/jquery-1.7.2.min.js"></script><script src="ui/js/jquery/jquery.ui.core.js"></script><script src="ui/js/jquery/jquery.ui.widget.js"></script><script src="ui/js/jquery/jquery.ui.mouse.js"></script><script src="ui/js/jquery/jquery.ui.button.js"></script><script src="ui/js/jquery/jquery.ui.draggable.js"></script><script src="ui/js/jquery/jquery.ui.position.js"></script><script src="ui/js/jquery/jquery.ui.resizable.js"></script><script src="ui/js/jquery/jquery.ui.button.js"></script><script src="ui/js/jquery/jquery.ui.dialog.js"></script><script src="ui/js/jquery/jquery.ui.effect.js"></script><script type="text/javascript" src="ui/js/pageUtils.js"></script><script type="text/javascript" src="ui/js/industry.js"></script><!--[if lt IE 9]><link rel="stylesheet" type="text/css" href="css/ie.css" /><![endif]--></head><body>	<div id="wrapper">		<div id="content">			<div class="c1">				<%@include file="common/head.jsp"%>				<div id="right_content"					style="margin-left: 150px;margin-right: 150px;">					<h2>						<label>行业管理</label> <label style="float: right;"><a							href="javascript:void(0);" class="button greens" id="addIndustry">添加行业</a>						</label>					</h2>					<table id="rounded-corner">						<thead>							<tr>								<th width="5%"></th>								<th width="20%">行业</th>								<th width="65%">备注</th>								<th width="5%">编辑</th>								<th width="5%">删除</th>							</tr>						</thead>						<tfoot>							<tr>								<td colspan="12">									<div class="form_sub_buttons">										<a href="#" class="button green" style="float: left;">全选</a> <a											href="#" class="button red" style="float: left;">删除选中的</a>									</div>									<div style="width: 400px;margin-top: 10px;float: right;">										<input type="hidden" value="${currentPage}" id="current">										<input type="hidden" value="${pageAll}" id="pageAll">										当前第${currentPage}页&nbsp;&nbsp;一共${pageAll}页&nbsp;&nbsp; <input											type="button" value="上一页" onclick="page.per();" />&nbsp;&nbsp;										<input type="button" value="下一页" onclick="page.next()" />&nbsp;&nbsp;										<input type="text" style="width: 30px;"											value="${currentPage }" name="currentPage" />&nbsp;&nbsp; <input											type="button" value="确认" onclick="page.jump();" />&nbsp;&nbsp;									</div>								</td>							</tr>						</tfoot>						<tbody>							<c:forEach items="${industryList}" var="u" varStatus="status">								<tr									<c:if test="${status.count%2 == 1}">								   class="even"								</c:if>									<c:if test="${status.count%2 == 0}">									class="odd"								</c:if>>									<td><input type="checkbox" name="" /></td>									<td>${u.industryName}</td>									<td>${u.note}</td>									<td><a href="javascript:void(0);" id="edit"										onclick="edit('${u.uuid}','${u.industryName}','${u.note}')"><img src="ui/images/edit.png"											alt="" title="" border="0" /> </a></td>									<td><a href="javascript:void(0);" id="trash"										onclick="deleteInds('${u.uuid}','${u.industryName}','${u.note}')"><img											src="ui/images/trash.gif" alt="" title="" border="0" /> </a></td>								</tr>							</c:forEach>						</tbody>					</table>				</div>			</div>		</div>		<%@include file="common/left.jsp"%>	</div><div id="dialog-form" title="添加行业">	<p class="validateTips"></p>	<form>	<fieldset>	    <p>		<label for="name">行业:</label>		<input type="text" name="industryName" id="industryName" style="width: 80%; height: 20px;" class="text ui-widget-content ui-corner-all"/>		</p>		<p>		<label for="note">备注:</label>		<textarea id="note" style="width: 80%;" rows="5" class="text ui-widget-content ui-corner-all" ></textarea>	    </p>	</fieldset>	</form></div></body></html>