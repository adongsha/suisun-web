<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%><%@ taglib uri="/WEB-INF/tlds/c.tld" prefix="c"%><%@ taglib prefix="form" uri="http://www.springframework.org/tags/form"%><%	String path = request.getContextPath();			String basePath = request.getScheme() + "://"					+ request.getServerName() + ":" + request.getServerPort()					+ path + "/";%><head><base href="<%=basePath%>" /><meta charset="utf-8"><title>随商管理后台</title><link media="all" rel="stylesheet" type="text/css"	href="ui/css/main.css" /><link media="all" rel="stylesheet" type="text/css"	href="ui/css/table.css" /><link rel="stylesheet" href="ui/js/jquery/css/jquery-ui.css"><script type="text/javascript" src="ui/js/jquery/jquery-1.7.2.min.js"></script><script type="text/javascript" src="ui/js/tip.js"></script><!--[if lt IE 9]><link rel="stylesheet" type="text/css" href="css/ie.css" /><![endif]--></head><body>	<div id="wrapper">		<div id="content">			<div class="c1">				<%@include file="common/head.jsp"%>				<div id="right_content"					style="margin-left: 150px;margin-right: 150px;">					<article>						<div class="text-section">							<h2>消息提示页</h2>							<p></p>						</div>						<input type="hidden" value="${code}" id="code"/>						<ul class="states">							<c:if test="${code == 1 }">								<li class="succes">成功 : 添加用户成功...</li>								<div class="form_sub_buttons">									<a href="u/adminAction.htm?method=forwardAddUser"										class="button green" style="float: left;">继续添加用户</a>								</div>							</c:if>							<c:if test="${code == -1 }">								<li class="error">失败 : 添加用户失败...</li>							</c:if>							<c:if test="${code == 2 }">								<li class="succes">成功 : 修改用户成功...</li>								<div class="form_sub_buttons">									<a href="u/adminAction.htm?method=editUser&id=${id}"										class="button green" style="float: left;">重新修改用户</a>								</div>							</c:if>							<c:if test="${code == -2 }">								<li class="error">失败 : 修改用户失败...</li>							</c:if>							<c:if test="${code == 3 }">								<li class="succes">成功 : 设置推荐画册成功...</li>								<div class="form_sub_buttons">									<a href="u/recommendAction.htm?method=forwardRecommend"										class="button green" style="float: left;">重新设置推荐画册</a>								</div>							</c:if>							<c:if test="${code == -3 }">								<li class="error">失败 : 设置推荐画册失败...</li>								<div class="form_sub_buttons">									<a href="u/recommendAction.htm?method=forwardRecommend"										class="button green" style="float: left;">重新设置推荐画册</a>								</div>							</c:if>						</ul>					</article>				</div>			</div>		</div>		<%@include file="common/left.jsp"%>	</div></html>